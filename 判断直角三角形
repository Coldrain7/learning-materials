#include <stdio.h>
int main()
{
	 int a,b,c;
	int n,i;
	scanf("%d",&n);
	for(i=0;i<n;i++){
		scanf("%d %d %d",&a,&b,&c);
		if(a<=0||b<=0||c<=0){
			printf("No\n");
		}else if(a*a+b*b==c*c){
		printf("Yes\n"); 
	}else if(a*a+c*c==b*b){
		printf("Yes\n");
	}else if(b*b+c*c==a*a){
		printf("Yes\n");
	}else printf("No\n");
}
	return 0;
}
