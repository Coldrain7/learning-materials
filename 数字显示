#include <stdio.h>
int main()
{
	int n,x,i,m,k,y;
	char a0[7][50]={"*****    ************   **************************",
                    "*   *    *    *    **   **    *        **   **   *",
	                "*   *    *    *    **   **    *        **   **   *",
	                "*   *    **************************    ***********",
	                "*   *    **        *    *    **   *    **   *    *",
	                "*   *    **        *    *    **   *    **   *    *",
	                "*****    ***********    ***********    ***********"};
	int a[4],z[4];
	scanf("%d",&n); 
	for(i=0;i<n;i++){
		m=0;
		scanf("%d",&x);//如输入321 
		printf("%d:\n",x);
		for(k=0;k<4;k++){
			a[m]=x%10;
			x=x/10;
			m++;//a0=1,a1=2,a2=3 
			if(x==0)break;
		}
		    
			for(y=0;y<7;y++){
				for(k=m-1;k>=0;k--){
				printf("%c%c%c%c%c  ",a0[y][5*a[k]],a0[y][5*a[k]+1],a0[y][5*a[k]+2],a0[y][5*a[k]+3],a0[y][5*a[k]+4]);
			}
			printf("\n");
			}
		}
	return 0;
}
